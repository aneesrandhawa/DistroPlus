<%- include('../partials/adminHeader.ejs') -%>
    <!--start page wrapper -->
    <div class="page-wrapper">
        <div class="page-content">
            <!--breadcrumb-->
            <div class="page-breadcrumb d-none d-sm-flex align-items-center mb-3">
                <div class="breadcrumb-title pe-3">Tables</div>
                <div class="ps-3">
                    <nav aria-label="breadcrumb">
                        <ol class="breadcrumb mb-0 p-0">
                            <li class="breadcrumb-item"><a href="javascript:;"><i class="bx bx-home-alt"></i></a>
                            </li>
                            <li class="breadcrumb-item active" aria-current="page">Data Table</li>
                        </ol>
                    </nav>
                </div>
                <div class="ms-auto">
                    <div class="btn-group">
                        <button type="button" class="btn btn-primary">Settings</button>
                        <button type="button" class="btn btn-primary split-bg-primary dropdown-toggle dropdown-toggle-split" data-bs-toggle="dropdown">	<span class="visually-hidden">Toggle Dropdown</span>
							</button>
                        <div class="dropdown-menu dropdown-menu-right dropdown-menu-lg-end"> <a class="dropdown-item" href="javascript:;">Action</a>
                            <a class="dropdown-item" href="javascript:;">Another action</a>
                            <a class="dropdown-item" href="javascript:;">Something else here</a>
                            <div class="dropdown-divider"></div> <a class="dropdown-item" href="javascript:;">Separated link</a>
                        </div>
                    </div>
                </div>
            </div>
            <!--end breadcrumb-->


            <h6 class="mb-0 text-uppercase">Database Products</h6>
            <hr/>
            <div class="card">
                <div class="card-body">
                    <div class="table-responsive">
                        <table id="example2" class="table table-striped table-bordered">
                            <thead>
                                <tr>
                                    <th>CATEGORY</th>
                                    <th>NAME</th>
                                    <th>PRICE</th>
                                    <th>FEATURE</th>
                                    <th>DISCRIPTION</th>
                                    <th>IMAGE</th>
                                    <th>Edit</th>
                                    <th>Del</th>
                                    <th>View</th>
                                </tr>
                            </thead>
                            <tbody>
                                <tr>
                                    <% result.forEach((e)=>{ %>
                                        <tr>
                                            <td>
                                                <% cat_result.forEach((cres)=>{if(e.cat_id==cres.cat_id){ab=cres.cat_name}}) %>

                                                    <%=ab%>
                                            </td>
                                            <td>
                                                <%=e.pro_name%>
                                            </td>
                                            <td>
                                                <%=e.pro_price%>
                                            </td>
                                            <td>
                                                <%=e.pro_featured%>
                                            </td>
                                            <td>
                                                <%=e.pro_desc%>
                                            </td>
                                            <td>
                                                <img src="/data/uploads/<%=e.pro_img%>" alt="" style="height:35px ;width:50px ;">

                                            </td>
                                            <td><a href="http://localhost:3000/admin/upro/<%=e.pro_id%>"><button >Edit</button></a></td>
                                            <td><a href="http://localhost:3000/admin/dspro/<%=e.pro_id%>"><button >Del</button></a></td>
                                            <td><a href="http://localhost:3000/admin/vspro/<%=e.pro_id%>"><button >View</button></a></td>
                                            <td>
                                        </tr>

                                        <% }) %>
                                </tr>
                            </tbody>
                        </table>
                    </div>
                </div>
            </div>
        </div>
    </div>
    <!--end page wrapper -->
    <%- include('../partials/adminFooter.ejs') -%>